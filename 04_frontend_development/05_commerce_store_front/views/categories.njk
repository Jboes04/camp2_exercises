{% extends "layout.njk" %}

{% block content %}
<div class="container">
<table class="table table-striped">
<thead class="thead-dark">
    <tr>
      <th scope="col">Categories</th>
    </tr>
</thead>
<tbody>
  {% for category in categories %}
  <tr>
    <td><a href="/category/{{category.id}}"> {{category.label}}</a></td>
  </tr>
  {% endfor %}
  
</tbody>
</table>
</div>
{% endblock %}
